<project name="${project.name}"
    xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
    xmlns='http://maven.apache.org/DECORATION/1.1.0'
    xsi:schemaLocation='http://maven.apache.org/DECORATION/1.1.0 http://maven.apache.org/xsd/decoration-1.1.0.xsd'>
    <skin>
        <groupId>org.apache.maven.skins</groupId>
        <artifactId>maven-fluido-skin</artifactId>
        <version>1.3.0</version>
    </skin>
    <custom>
        <fluidoSkin>
            <topBarEnabled>true</topBarEnabled>
            <sideBarEnabled>false</sideBarEnabled>
        </fluidoSkin>
    </custom>
    <version position="right" />
    <publishDate position="bottom" />
    <body>
        <breadcrumbs>
            <item name="${project.name}" href="/index.html" />
        </breadcrumbs>
        <menu name="Overview">
            <item name="About" href="index.html" />
            <item name="Resource Caching Tutorial" href="resource-caching.html" />
            <item name="Performance Tuning" href="performance-tuning.html" />
        </menu>
        <menu ref="reports" />
        <links>
            <item href="http://www.ehcache.org/" alt="Ehcache" name="Ehcache" target="_blank" />
            <item href="http://sworisbreathing.github.com/sfmf4j/" alt="SFMF4J: Simple File Monitor Facade for Java" name="SFMF4J" target="_blank"/>
        </links>
    </body>
</project>
